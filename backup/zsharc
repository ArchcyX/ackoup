_cp() {
    mkdir "$1" && cd "$1" && \
    mkdir src includes && \
    touch Makefile main.c && \
    echo "$banner" > TODO_list && \
	echo -e "$YELLOW Creating the Project.. $RED => '$1' <= $RESET" && \
	echo -e " $GREEN built compiled ðŸ‘¿ \n $RESET" && \
}

# cp fonksiyonunun alias'Ä±nÄ± bu ÅŸekilde oluÅŸturuyoruz.
